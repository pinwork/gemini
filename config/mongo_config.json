{
  "client_params": {
    "serverSelectionTimeoutMS": 30000,
    "connectTimeoutMS": 30000,
    "socketTimeoutMS": 30000,
    "maxPoolSize": 300,
    "minPoolSize": 50,
    "maxIdleTimeMS": 30000,
    "waitQueueTimeoutMS": 5000,
    "retryWrites": true,
    "retryReads": true
  },
  "databases": {
    "main_db": {
      "uri": "mongodb://admin:*@*:27017/?authSource=admin",
      "name": "webinfo",
      "collections": {
        "domain_main": "domain_main",
        "domain_segmented": "domain_segmented",
        "gemini": "gemini",
        "gemini_address_list": "gemini_address_list",
        "gemini_email_list": "gemini_email_list",
        "gemini_phone_list": "gemini_phone_list"
      }
    },
    "api_db": {
      "uri": "mongodb://admin:*@*:27017/?authSource=admin",
      "name": "api",
      "collections": {
        "keys": "data"
      }
    }
  }
}
